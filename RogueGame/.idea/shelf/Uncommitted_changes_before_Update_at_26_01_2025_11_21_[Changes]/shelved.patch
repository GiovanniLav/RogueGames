<+>/* Impostazioni generali per il layout */\r\nbody {\r\n    font-family: Arial, sans-serif;\r\n    background-color: #f4f4f4;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.pul {\r\n    border-radius: 5px;\r\n    background-color: #222;\r\n    color: #ff9900;\r\n}\r\n\r\n/* Header */\r\nheader {\r\n    background-color: #333;\r\n    color: #fff;\r\n    padding: 10px;\r\n    text-align: center;\r\n}\r\n\r\nheader a {\r\n    color: #fff;\r\n    text-decoration: none;\r\n    font-size: 1.5rem;\r\n}\r\n\r\n/* Stile per la lista dei prodotti */\r\n.prodotti-container {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    gap: 20px;\r\n    justify-content: center;\r\n    margin: 20px;\r\n}\r\n\r\n/* Ogni card di prodotto */\r\n.prodotto-card {\r\n    background-color: #fff;\r\n    border: 1px solid #ddd;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n    width: 250px;\r\n    padding: 15px;\r\n    text-align: center;\r\n    transition: transform 0.3s ease;\r\n}\r\n\r\n.prodotto-card:hover {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.prodotto-card img {\r\n    max-width: 100%;\r\n    height: auto;\r\n    border-radius: 5px;\r\n}\r\n\r\n.prodotto-card h3 {\r\n    font-size: 1.25rem;\r\n    margin: 10px 0;\r\n}\r\n\r\n.prodotto-card p {\r\n    font-size: 1rem;\r\n    color: #666;\r\n    margin: 10px 0;\r\n}\r\n\r\n.prodotto-card .prezzo {\r\n    font-size: 1.2rem;\r\n    color: #28a745;\r\n    font-weight: bold;\r\n    margin: 10px 0;\r\n}\r\n\r\n/* Pulsanti per modifica ed eliminazione */\r\n.prodotto-card .actions {\r\n    margin-top: 15px;\r\n}\r\n\r\n.prodotto-card .btn {\r\n    padding: 8px 15px;\r\n    border: none;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    margin-right: 5px;\r\n}\r\n\r\n.prodotto-card .btn:hover {\r\n    background-color: #0056b3;\r\n}\r\n\r\n/* Stile per il pulsante di aggiunta prodotto */\r\n.add-product-btn {\r\n    background-color: #28a745;\r\n    color: white;\r\n    padding: 10px 20px;\r\n    border-radius: 5px;\r\n    text-align: center;\r\n    font-size: 1.1rem;\r\n    margin: 20px auto;\r\n    display: block;\r\n    cursor: pointer;\r\n}\r\n\r\n.add-product-btn:hover {\r\n    background-color: #218838;\r\n}\r\n\r\n/* Modal (Popup) per aggiungere prodotto */\r\n.modal {\r\n    display: none;\r\n    position: fixed;\r\n    z-index: 1;\r\n    left: 0;\r\n    top: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    overflow: auto;\r\n    background-color: rgb(0, 0, 0);\r\n    background-color: rgba(0, 0, 0, 0.4);\r\n    padding-top: 60px;\r\n}\r\n\r\n.modal-content {\r\n    background-color: #fff;\r\n    margin: 5% auto;\r\n    padding: 20px;\r\n    border: 1px solid #888;\r\n    width: 50%;\r\n    border-radius: 10px;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.modal-header {\r\n    font-size: 1.5rem;\r\n    margin-bottom: 15px;\r\n}\r\n\r\n.modal-body input, .modal-body select, .modal-body textarea {\r\n    width: 100%;\r\n    padding: 8px;\r\n    margin: 10px 0;\r\n    border-radius: 5px;\r\n    border: 1px solid #ddd;\r\n}\r\n\r\n.modal-footer {\r\n    display: flex;\r\n    justify-content: flex-end;\r\n}\r\n\r\n.modal-footer .btn {\r\n    background-color: #28a745;\r\n    color: white;\r\n    padding: 10px 15px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.modal-footer .btn:hover {\r\n    background-color: #218838;\r\n}\r\n\r\n/* Stile per il pulsante di chiusura della modal */\r\n.close {\r\n    color: #aaa;\r\n    font-size: 30px;\r\n    font-weight: bold;\r\n    float: right;\r\n    cursor: pointer;\r\n}\r\n\r\n.close:hover,\r\n.close:focus {\r\n    color: black;\r\n    text-decoration: none;\r\n    cursor: pointer;\r\n}\r\n\r\ntable {\r\n    margin: 0 auto 20px auto;\r\n    border-collapse: collapse;\r\n    border: 3px solid white;\r\n    width: 100%;\r\n}\r\n\r\nth, td {\r\n    border: 1px solid white;\r\n    padding: 10px;\r\n    text-align: center;\r\n}\r\n\r\nth {\r\n    background-color: darkgray;\r\n}\r\n\r\ntd {\r\nbackground-color: lightgray;\r\n}\r\n\r\nh1 {\r\n    color: #ff9900;\r\n    text-align: center;\r\n}

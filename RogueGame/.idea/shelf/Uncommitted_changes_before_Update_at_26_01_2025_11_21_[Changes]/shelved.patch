<+>/* Impostazioni di base */\r\nbody {\r\n    font-family: 'Roboto', sans-serif;\r\n    background: linear-gradient(135deg, #ff7e5f, #feb47b);\r\n    height: 100vh;\r\n    margin: 0;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    color: #fff;\r\n    overflow: hidden;\r\n}\r\n\r\n.container {\r\n    background-color: rgba(0, 0, 0, 0.7);\r\n    padding: 25px;\r\n    border-radius: 8px;\r\n    width: 100%;\r\n    max-width: 400px;\r\n    text-align: center;\r\n    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);\r\n    transition: transform 0.3s ease-in-out;\r\n}\r\nh2 {\r\n    font-family: 'Open Sans', sans-serif;\r\n    font-size: 2em;\r\n    color: #fff;\r\n    margin-bottom: 20px;  /* Aggiungi un po' di spazio tra il titolo e il form */\r\n    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.7);\r\n}\r\n\r\n\r\nform {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\nlabel {\r\n    font-size: 1.1em;\r\n    margin-top: 0px;  /* Ridotto margine */\r\n    text-align: left;\r\n}\r\n\r\ninput {\r\n    padding: 0px;  /* Ridotto padding */\r\n    margin: 0px 0;  /* Ridotto margine tra gli input */\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-size: 1em;\r\n    outline: none;\r\n    transition: all 0.3s ease;\r\n}\r\n\r\ninput:focus {\r\n    border: 2px solid #ff7e5f;\r\n    box-shadow: 0 0 10px rgba(255, 126, 95, 0.7);\r\n}\r\n\r\nbutton {\r\n    padding: 10px;\r\n    background-color: #ff7e5f;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    font-size: 1.1em;\r\n    cursor: pointer;\r\n    transition: background-color 0.3s ease;\r\n}\r\n\r\nbutton:hover {\r\n    background-color: #feb47b;\r\n}\r\n\r\n.error {\r\n    color: #ff4d4d;\r\n    font-size: 0.9em;\r\n    margin-top: 2px;  /* Ridotto margine */\r\n}\r\n\r\n/* Animazione per il form */\r\n.container:hover {\r\n    transform: translateY(-10px);\r\n}\r\n\r\n/* Media query per dispositivi mobili */\r\n@media (max-width: 600px) {\r\n    .container {\r\n        padding: 20px;\r\n        width: 90%;\r\n    }\r\n\r\n    h1 {\r\n        font-size: 2em;\r\n    }\r\n\r\n    label {\r\n        font-size: 1em;\r\n    }\r\n\r\n    input, button {\r\n        font-size: 1em;\r\n    }\r\n}\r\n

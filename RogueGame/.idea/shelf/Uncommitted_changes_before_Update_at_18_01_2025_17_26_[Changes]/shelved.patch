<+>body {\r\n    font-family: Arial, sans-serif;\r\n    background-color: #f4f4f4;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.company-name {\r\n    text-align: center;\r\n    font-size: 30px;\r\n    font-weight: bold;\r\n    color: #ff007f;\r\n}\r\n.adminBtn {\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    padding: 5px 10px;\r\n    text-decoration: none;\r\n    border-radius: 5px;\r\n    font-size: 0.9em;\r\n    transition: background-color 0.3s;\r\n}\r\n\r\n#carrello {\r\n    background: #00bfff;\r\n    color: white;\r\n}\r\n\r\n.company-name .highlight {\r\n    color: #00bfff;\r\n}\r\n\r\n.catalogo {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-around; /* Spaziatura tra i prodotti */\r\n    padding: 20px; /* Padding esterno */\r\n}\r\n\r\n.prodotto {\r\n    width: calc(25% - 20px); /* Larghezza del prodotto - considera il padding e lo spazio tra i prodotti */\r\n    background-color: #fff;\r\n    border: 1px solid #ddd;\r\n    border-radius: 5px;\r\n    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\r\n    padding: 15px;\r\n    text-align: center;\r\n    transition: transform 0.2s ease-in-out;\r\n    margin-bottom: 20px; /* Spazio sotto ogni prodotto */\r\n}\r\n\r\n.prodotto img {\r\n    border-radius: 5px;\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.prodotto h3 {\r\n    font-size: 1.2em;\r\n    margin: 10px 0;\r\n}\r\n\r\n.prodotto .descrizione {\r\n    color: #777;\r\n    font-size: 0.9em;\r\n    margin: 10px 0;\r\n}\r\n\r\n.prodotto .prezzo {\r\n    color: #333;\r\n    font-size: 1.1em;\r\n    font-weight: bold;\r\n    margin: 10px 0;\r\n}\r\n\r\n.prodotto:hover {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.star {\r\n    font-size: 30px;\r\n    color: #ccc;\r\n    background: none;\r\n    border: none;\r\n    cursor: pointer;\r\n    transition: color 0.3s ease;\r\n    /*align-self: right;*/\r\n}\r\n\r\n.star:focus {\r\n    outline: none;\r\n}\r\n\r\n.active {\r\n    color: #FDD017;\r\n}\r\n\r\n/* Media Queries per Schermi Mobili e Tablet */\r\n@media (max-width: 1200px) {\r\n    .prodotto {\r\n        width: calc(33.333% - 20px); /* 3 colonne */\r\n    }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n    .prodotto {\r\n        width: calc(50% - 20px); /* 2 colonne */\r\n    }\r\n}\r\n\r\n@media (max-width: 480px) {\r\n    .prodotto {\r\n        width: 100%; /* 1 colonna */\r\n    }\r\n}\r\n
